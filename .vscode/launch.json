{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug FyLibTest",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/FyLibTest/bin/Debug/net10.0/FyLibTest.dll",
            "args": [],
            "cwd": "${workspaceFolder}/FyLibTest",
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "Debug FyLibTest (external terminal)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/FyLibTest/bin/Debug/net10.0/FyLibTest.dll",
            "args": [],
            "cwd": "${workspaceFolder}/FyLibTest",
            "console": "externalTerminal",
            "stopAtEntry": false
        },
        {
            "name": "Launch FyLibTest with dotnet",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "dotnet",
            "args": [
                "${workspaceFolder}/FyLibTest/bin/Debug/net10.0/FyLibTest.dll"
            ],
            "cwd": "${workspaceFolder}/FyLibTest",
            "console": "internalConsole",
            "stopAtEntry": false
